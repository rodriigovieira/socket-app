{
    "extends": "airbnb-base",
    "rules": {
        "no-underscore-dangle": 1,
        "consistent-return": 1,
        "no-unused-vars": 1,
        "comma-dangle": 1,
        "object-curly-newline": 1,
        "semi": 1,
        "no-console": 0
    },
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "shared-node-browser": false,
        "worker": false,
        "amd": false,
        "mocha": true,
        "jasmine": true,
        "jest": true,
        "phantomjs": false,
        "jquery": false,
        "qunit": false,
        "prototypejs": false,
        "shelljs": false,
        "meteor": false,
        "mongo": true,
        "protractor": false,
        "applescript": false,
        "nashorn": false,
        "serviceworker": false,
        "atomtest": false,
        "embertest": false,
        "webextensions": false,
        "es6": true,
        "greasemonkey": false
    }
}